{
  "expo": {
    "name": "KonterApp",
    "slug": "konter-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "android",
      "ios"
    ],
    "android": {
      "package": "com.konterapp.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "extra": {
      "eas": {
        "projectId": "07b129e3-58be-4a62-b00a-f179315e7382"
      }
    },
    "ios": {
      "bundleIdentifier": "com.konterapp.app"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
